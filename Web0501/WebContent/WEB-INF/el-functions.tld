<?xml version="1.0" encoding="euc-kr" ?>

<taglib xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
    	http://java.sun.com/xml/ns/javaee/web-jsptaglibrary_2_1.xsd"
	version="2.1">

	<description>EL에서 함수실행</description>
	<tlib-version>1.0</tlib-version>
	<short-name>ELfunctions</short-name>
	<uri>/ELFunctions</uri>

	<function>
		<description>Date 객체 포맷팅</description>
		<name>dateFormat</name>
		<function-class>util.el.DateUtil</function-class>
		<function-signature>
			java.lang.String asd(java.util.Date)
		</function-signature>
	</function>
	
	
	<function>
		<description>comment_count</description>
		<name>getCommentCount</name>
		<function-class>comment.CommentDBBean</function-class>
		<function-signature>
			int getCommentCount(int)
		</function-signature>
	</function>
	
</taglib>
